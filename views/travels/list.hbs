<div class="row">
{{#each travels as |travel|}} 
<div class="col-6">


<div class="card m-2">
  <div class="row g-0">
    <div class="col-md-4">
      
      <img src="{{travel.image}}" class="img-fluid rounded-start" alt="...">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title">{{travel.title}}</h5>
        <p class="card-text">{{travel.description}}</p>

        <ul>
          <li>{{travel.tripMode}}</li>
          <li>{{travel.duration}}</li>
          <li>{{#setKeyWords travel.keyWords}}{{/setKeyWords}}</li>
          
          
        </ul>
       
    

        <h4 class="card-title">{{travel.price}}â‚¬</h4>

        <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
      </div>
    </div>
  </div>
</div>
</div>
{{/each}}
</div>